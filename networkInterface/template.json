{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "networkInterfaces_vml_learnthree_westeurope_dev324_name": {
            "defaultValue": "vml-learnthree-westeurope-dev324",
            "type": "String"
        },
        "virtualNetworks_vml_learn_westeurope_dev_vnet_externalid": {
            "defaultValue": "/subscriptions/56a4394e-6c1e-4255-958f-17a40db529a5/resourceGroups/rg-learn-westeurope-dev/providers/Microsoft.Network/virtualNetworks/vml-learn-westeurope-dev-vnet",
            "type": "String"
        },
        "networkSecurityGroups_vml_learnthree_westeurope_dev_nsg_externalid": {
            "defaultValue": "/subscriptions/56a4394e-6c1e-4255-958f-17a40db529a5/resourceGroups/rg-newmayank-westeurope-dev/providers/Microsoft.Network/networkSecurityGroups/vml-learnthree-westeurope-dev-nsg",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Network/networkInterfaces",
            "apiVersion": "2024-03-01",
            "name": "[parameters('networkInterfaces_vml_learnthree_westeurope_dev324_name')]",
            "location": "westeurope",
            "kind": "Regular",
            "properties": {
                "ipConfigurations": [
                    {
                        "name": "ipconfig1",
                        "id": "[concat(resourceId('Microsoft.Network/networkInterfaces', parameters('networkInterfaces_vml_learnthree_westeurope_dev324_name')), '/ipConfigurations/ipconfig1')]",
                        "type": "Microsoft.Network/networkInterfaces/ipConfigurations",
                        "properties": {
                            "privateIPAddress": "10.0.0.6",
                            "privateIPAllocationMethod": "Dynamic",
                            "subnet": {
                                "id": "[concat(parameters('virtualNetworks_vml_learn_westeurope_dev_vnet_externalid'), '/subnets/default')]"
                            },
                            "primary": true,
                            "privateIPAddressVersion": "IPv4",
                            "loadBalancerBackendAddressPools": [
                                {
                                    "id": "[concat(parameters('loadBalancers_lbe_learn_westeurope_dev_externalid'), '/backendAddressPools/bp-learn-westeurope-dev')]"
                                }
                            ]
                        }
                    }
                ],
                "dnsSettings": {
                    "dnsServers": []
                },
                "enableAcceleratedNetworking": false,
                "enableIPForwarding": false,
                "disableTcpStateTracking": false,
                "networkSecurityGroup": {
                    "id": "[parameters('networkSecurityGroups_vml_learnthree_westeurope_dev_nsg_externalid')]"
                },
                "nicType": "Standard",
                "auxiliaryMode": "None",
                "auxiliarySku": "None"
            }
        }
    ]
}